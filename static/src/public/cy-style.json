{
    "stylesheet": [
        {
            "selector": "node",
            "style": {
                "content": "data(_label)",
                "shape": "data(_shape)",
                "width": "90vw",
                "font-size": "10vh",
                "font-weight": "bold",
                "text-wrap": "wrap",
                "text-overflow-wrap": "whitespace",
                "text-max-width": "80vw",
                "background-color": "white",
                "border-color": "#777",
                "border-width": "1vw",
                "border-style": "solid"
            }
        },
        {
            "selector": ".schema",
            "style": {
                "background-color": "3d3fc6"
            }
        },
        {
            "selector": "node[^_shape]",
            "style": {
                "shape": "round-rectangle"
            }
        },
        {
            "selector": "node[^_label]",
            "style": {
                "content": ""
            }
        },
        {
            "selector": "node[_type = 'root']",
            "style": {
                "background-color": "#3d3fc6"
            }
        },
        {
            "selector": "node[_type = 'participant']",
            "style": {
                "background-color": "gray"
            }
        },
        {
            "selector": "node[_type = 'child']",
            "style": {
                "background-color": "#f1c232"
            }
        },
        {
            "selector": "node:selected",
            "style": {
                "background-color": "gray"
            }
        },
        {
            "selector": "edge",
            "style": {
                "label": "data(_label)",
                "font-size": "12vh",
                "curve-style": "bezier",
                "target-arrow-shape": "triangle",
                "target-arrow-color": "#000000",
                "line-style": "solid",
                "edge-text-rotation": "autorotate"
            }
        },
        {
            "selector": "edge[_edge_type = 'step_child']",
            "style": {
                "line-color": "#189ad3",
                "target-arrow-shape": "none",
                "width": "1vw"
            }
        },
        {
            "selector": "edge[_edge_type = 'step_participant']",
            "style": {
                "target-arrow-color": "#777",
                "line-color": "#777",
                "line-style": "dashed",
                "width": "2vw"
            }
        },
        {
            "selector": "edge[_edge_type = 'child_outlink']",
            "style": {
                "line-color": "#000000",
                "width": "2vw"
            }
        },
        {
            "selector": "edge:selected",
            "style": {
                "line-color": "#91a59f",
                "target-arrow-color": "#91a59f"
            }
        },
        {
            "selector": ".optional-before",
            "style": {
                "line-style": "dashed"
            }
        }
    ],
    "layout": {
        "name": "klay",
        "nodeDimensionsIncludeLabels": false,
        "fit": true,
        "padding": 20,
        "animate": false,
        "animationDuration": 500,
        "klay": {
            "addUnnecessaryBendpoints": false,
            "aspectRatio": 1.6,
            "borderSpacing": 20,
            "compactComponents": false,
            "crossingMinimization": "LAYER_SWEEP",
            "cycleBreaking": "GREEDY",
            "direction": "RIGHT",
            "edgeRouting": "SPLINES",
            "edgeSpacingFactor": 0.7,
            "feedbackEdges": false,
            "fixedAlignment": "NONE",
            "inLayerSpacingFactor": 4,
            "layoutHierarchy": true,
            "linearSegmentsDeflectionDampening": 0.3,
            "mergeEdges": false,
            "mergeHierarchyCrossingEdges": true,
            "nodeLayering": "NETWORK_SIMPLEX",
            "nodePlacement": "LINEAR_SEGMENTS",
            "randomizationSeed": 10,
            "routeSelfLoopInside": false,
            "separateConnectedComponents": true,
            "spacing": 20,
            "thoroughness": 20
        }
    }
}